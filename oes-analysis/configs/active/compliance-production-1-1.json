{
    "transcripts_directory": "call-type-classified-monash-calls",
    "output_directory": "multi_agent_responses",
    "input_catalog": {
          "name": "requirement",
          "location": "compliance",
          "types": ["broad", "enhanced"],
          "logic": {
            "call_recording_disclosure": "lambda kwargs: int(kwargs['call_id']) in kwargs['get_pickle']('outbound')",
            "census": "lambda kwargs: kwargs['parent_dir'] == 'follow-up-enrollment'",
            "declaration": "lambda kwargs: kwargs['parent_dir'] == 'follow-up-enrollment'",
            "fees": "lambda kwargs: kwargs['parent_dir'] == 'follow-up-enrollment'",
            "units": "lambda kwargs: kwargs['parent_dir'] == 'follow-up-enrollment'"
          }
    },
    "nodes": [
        {
            "name": "check",
            "inputs": [
                "requirement-broad",
                "requirement-enhanced",
                "transcript"
            ],
            "models": [
                {
                    "provider": "azure"
                }
            ]
        },
        {
            "name": "errors",
            "inputs": [
                "requirement-broad",
                "requirement-enhanced",
                "transcript",
                "check"
            ],
            "models": [
                {
                    "provider": "azure"
                }
            ]
        },
        {
            "name": "validation",
            "inputs": [
                "requirement-broad",
                "requirement-enhanced",
                "transcript",
                "check",
                "errors"
            ],
            "models": [
                {
                    "provider": "azure"
                }
            ]
        }
    ]
}