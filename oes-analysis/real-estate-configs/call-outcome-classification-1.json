{
    "transcripts_directory": "real-estate-calls",
    "output_directory": "multi_agent_responses",
    "input_catalog": {
        "name": "call_outcome",
        "location": "real-estate-definitions",
        "types": ["taxonomy"]
    },
    "nodes": [
        {
            "name": "outcome_classification",
            "inputs": [
                "call_outcome-taxonomy",
                "transcript"
            ],
            "models": [
                {"provider": "azure", "mini": true},
                {"provider": "azure", "mini": true},
                {"provider": "azure", "mini": true}
            ],
            "response_format_schema": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "call_outcome": {
                        "type": "object",
                        "properties": {
                            "primary_outcome": {
                                "type": "string",
                                "enum": ["showing_scheduled", "offer_discussion", "follow_up_scheduled", "information_provided", "objection_unresolved", "lost_lead", "referral_made", "other"]
                            },
                            "success_indicator": {"type": "boolean"},
                            "next_steps_committed": {
                                "type": "array",
                                "items": {"type": "string"}
                            },
                            "buyer_engagement_level": {
                                "type": "string",
                                "enum": ["highly_engaged", "moderately_engaged", "minimally_engaged", "disengaged"]
                            },
                            "evidence": {"type": "string"}
                        },
                        "required": ["primary_outcome", "success_indicator", "buyer_engagement_level", "evidence"]
                    }
                },
                "required": ["call_outcome"]
            }
        },
        {
            "name": "aggregation",
            "inputs": [
                "call_outcome-taxonomy",
                "transcript",
                "outcome_classification"
            ],
            "models": [
                {"provider": "azure", "mini": true}
            ],
            "response_format_schema": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "call_outcome": {
                        "type": "object",
                        "properties": {
                            "primary_outcome": {"type": "string"},
                            "consensus_count": {"type": "integer", "minimum": 1, "maximum": 3},
                            "success_indicator": {"type": "boolean"},
                            "next_steps_committed": {"type": "array", "items": {"type": "string"}},
                            "buyer_engagement_level": {"type": "string"},
                            "evidence": {"type": "string"},
                            "confidence": {"type": "number", "minimum": 0, "maximum": 1}
                        },
                        "required": ["primary_outcome", "consensus_count", "success_indicator", "buyer_engagement_level", "confidence"]
                    }
                },
                "required": ["call_outcome"]
            }
        }
    ]
}
